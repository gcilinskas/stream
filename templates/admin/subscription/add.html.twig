{% extends 'admin/base.html.twig' %}
{% block body %}
    <div id="content-page" class="content-page">
        <div class="container-fluid">
            <div class="row">
                <div class="col-sm-12">
                    <div class="iq-card">

                        <div class="iq-card-header d-flex justify-content-between">
                            <div class="iq-header-title">
                                <h4 class="card-title">Pridėti Prenumeratą</h4>
                            </div>
                        </div>

                        <div class="iq-card-body">
                            {{ form_start(form) }}
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="row">

                                        <div class="col-12 form-group">
                                            {{ form_label(form.validFrom) }}
                                            {{ form_widget(form.validFrom) }}
                                            {{ form_errors(form.validFrom) }}
                                        </div>

                                        <div class="col-12 form-group">
                                            {{ form_label(form.validTo) }}
                                            {{ form_widget(form.validTo) }}
                                            {{ form_errors(form.validTo) }}
                                        </div>

                                        <div class="col-12 form-group">
                                            {{ form_label(form.userEmail) }}
                                            {{ form_widget(form.userEmail) }}
                                            {{ form_errors(form.userEmail) }}
                                        </div>

                                        <div class="form-group">
                                            {{ form_widget(form.submit) }}
                                        </div>

                                    </div>
                                </div>
                            </div>
                            {{ form_end(form) }}
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
